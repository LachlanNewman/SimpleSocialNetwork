<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.NewProfileController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <children>
            <TextField fx:id="firstNameField" promptText="First Name" />
            <TextField fx:id="lastNameField" promptText="Last Name">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></TextField>
            <TextField fx:id="ageField" promptText="Age">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></TextField>
            <HBox prefHeight="0.0" prefWidth="200.0">
               <children>
                  <CheckBox fx:id="maleCheckBox" mnemonicParsing="false" onAction="#configGenderCheckBoxes" prefWidth="100.0" text="MALE">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </CheckBox>
                  <CheckBox fx:id="femaleCheckbox" mnemonicParsing="false" onAction="#configGenderCheckBoxes" prefWidth="100.0" text="FEMALE" />
               </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </HBox>
            <TextField fx:id="statusField" promptText="Status">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </TextField>
            <ComboBox fx:id="stateField" prefWidth="200.0">
                <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="VIC" />
                    <String fx:value="WA" />
                    <String fx:value="NSW" />
                </FXCollections>
            </items>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </ComboBox>
             <Button fx:id="newProfileButton" onAction="#addNewProfile" prefWidth="200.0" text="Add Profile">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Button>
         </children></VBox>
   </children>
